main: 
	gcc ../src/adt/queue/queue.c ../src/adt/arraydin/arraydin.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/mesinkata/mesinkata.c ../src/adt/word/word.c ../src/adt/string/string.c ../src/adt/order/order.c ../src/games/dinerdash/dinerdash.c ../src/console.c ../src/main.c ../src/gambar.c -o main

dinerdash:
	gcc ../src/games/dinerdash/mdinerdash.c ../src/games/dinerdash/dinerdash.c ../src/adt/order/order.c ../src/adt/string/string.c ../src/adt/mesinkata/mesinkata.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/word/word.c ../src/gambar.c -o dinerdash

randomnumber:
	gcc ../src/games/rng/randomnumber.c ../src/games/rng/rng.c -o randomnumber

rng:
	gcc ../src/games/rng/rng.c -o rng

mesinkata:
	gcc ../src/adt/mesinkata/mmesinkata.c ../src/adt/mesinkata/mesinkata.c ../src/adt/string/string.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/word/word.c -o mesinkata