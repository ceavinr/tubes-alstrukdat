main: 
	gcc ../src/adt/queue/queue.c ../src/adt/arraydin/arraydin.c ../src/adt/array/array.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/mesinkata/mesinkata.c ../src/adt/word/word.c ../src/adt/string/string.c ../src/adt/order/order.c ../src/games/dinerdash/dinerdash.c ../src/games/tictactoe/tictactoe.c ../src/games/rng/rng.c ../src/console.c ../src/main.c ../src/gambar.c -o main

dinerdash:
	gcc ../src/games/dinerdash/mdinerdash.c ../src/adt/arraydin/arraydin.c ../src/games/dinerdash/dinerdash.c ../src/adt/order/order.c ../src/adt/string/string.c ../src/adt/mesinkata/mesinkata.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/word/word.c ../src/gambar.c -o dinerdash

rng:
	gcc ../src/games/rng/mrng.c ../src/games/rng/rng.c -o rng

mesinkata:
	gcc ../src/adt/mesinkata/mmesinkata.c ../src/adt/mesinkata/mesinkata.c ../src/adt/string/string.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/word/word.c -o mesinkata

word:
	gcc ../src/adt/word/mword.c ../src/adt/mesinkata/mesinkata.c ../src/adt/string/string.c ../src/adt/mesinkarakter/mesinkarakter.c ../src/adt/word/word.c -o word

tictactoe:
	gcc ../src/adt/array/array.c ../src/games/tictactoe/mtictactoe.c ../src/games/tictactoe/tictactoe.c -o tictactoe